option_settings:
  aws:elasticbeanstalk:container:python:
    WSGIPath: disc.wsgi:application
  aws:elasticbeanstalk:application:environment:
    LD_LIBRARY_PATH: "/usr/local/lib"
  aws:elasticbeanstalk:environment:proxy:staticfiles:
    /static: static
commands:
  01_upgrade_sqlite:
    command: "cd ~ && wget https://www.sqlite.org/2022/sqlite-autoconf-3390000.tar.gz && tar xvfz sqlite-autoconf-3390000.tar.gz && cd sqlite-autoconf-3390000 && ./configure && make && make install"